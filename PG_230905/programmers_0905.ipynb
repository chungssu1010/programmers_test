      },
      "outputs": [],
      "source": [
        "# 내 답\n",
        "\n",
        "def solution(s):\n",
        "    result = 0\n",
        "    count_zero = 0\n",
        "\n",
        "    while len(s) > 1:\n",
        "        count_zero += s.count('0')\n",
        "        s = bin(len(s) - s.count('0'))[2:]\n",
        "        result += 1\n",
        "\n",
        "    return [result, count_zero]"
      ]
    }
  ]
